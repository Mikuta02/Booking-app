<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="PrimaryBackground" Color="#27374D"/>
    <SolidColorBrush x:Key="SecondaryBackground" Color="#526D82"/>
    <SolidColorBrush x:Key="ButtonBackground" Color="#bccad3"/>
    <SolidColorBrush x:Key="BorderBrushColor" Color="#888888"/>
    <SolidColorBrush x:Key="SelectedTabBackground" Color="#3C5B6F"/>
    <SolidColorBrush x:Key="PrimaryTextColor" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="ButtonTextColor" Color="#000000"/>
    <SolidColorBrush x:Key="BlackTextColor" Color="#000000"/>
    <BitmapImage x:Key="LeftImageSource" UriSource="pack://application:,,,/Resources/Images/Tourist/left_light_theme.png"/>
    <BitmapImage x:Key="NextImageSource" UriSource="pack://application:,,,/Resources/Images/Tourist/next_light.png"/>
</ResourceDictionary>