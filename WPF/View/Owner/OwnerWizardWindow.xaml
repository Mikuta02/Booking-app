<Window x:Class="BookingApp.WPF.View.Owner.OwnerWizardWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BookingApp.WPF.View.Owner"
        mc:Ignorable="d"
        Title="OwnerWizardWindow" Height="600" Width="430" WindowStartupLocation="CenterOwner">

    <StackPanel>
        <Image Source="{Binding WizardImage}"></Image>
        <Button Content="{Binding ButtonContent}" Command="{Binding ButtonCommand}"></Button>
    </StackPanel>

</Window>
