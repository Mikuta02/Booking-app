<Window x:Class="BookingApp.WPF.View.Tourist.DelayInputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BookingApp.WPF.View.Tourist"
        xmlns:resources="clr-namespace:BookingApp"
        mc:Ignorable="d"
        Title="Kasnjenje" Height="150" Width="300"
        WindowStartupLocation="CenterScreen">
    <StackPanel Margin="10">
        <Label>Enter delay in minutes:</Label>
        <TextBox x:Name="DelayTextBox"/>
        <Button Content="Submit" Margin="5" Padding="3" Click="SubmitButton_Click"/>
    </StackPanel>
</Window>
